<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zioerjens.fitbet">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.Ac" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fitbet"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fitbet"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".Home" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TestJsonParse"
            android:label="@string/titelStat"
            android:screenOrientation="portrait"
            android:noHistory="true"/>
        <activity
            android:name=".tippen_Spielauswahl"
            android:label="@string/title_activity_tippen__spielauswahl"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Gruppenauswahl"
            android:label="@string/titelActGr"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".newGroupAct"
            android:label="@string/titelActNewGr"
            android:theme="@style/AppTheme.PopUpGroup"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Sport"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SignIn"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Tippen"
            android:screenOrientation="portrait"/>
        <activity android:name=".TippenAllGames"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GruppeDetail"
            android:label=""
            android:screenOrientation="portrait"/>
    </application>

</manifest>